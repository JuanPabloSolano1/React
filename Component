import React from 'react';
import ReactDOM from 'react-dom';

class First extends React.Component{
  render(){
    const header = "My fancy new product"
    const paragraph = "Test it now!"
    return(
    <div class="article"> 
      <h2>{header}</h2>
      <p>{paragraph}</p>
    </div> 
    )
  }
}
ReactDOM.render(<First />, document.getElementById("root"))
